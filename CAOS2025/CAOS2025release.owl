    </owl:Class>
    


    <!-- http://www.semanticweb.org/cdp#establishment_of_functional_structures -->

    <owl:Class rdf:about="http://www.semanticweb.org/cdp#establishment_of_functional_structures">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000011"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000132"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/cdp#conceptual_design_process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An establishment of functional structure is a planned process in which the overall function is subdivided into simpler functions, i.e., subfunctions, to yield a hierarchy of functions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/cdp#functional_analysis -->

    <owl:Class rdf:about="http://www.semanticweb.org/cdp#functional_analysis">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000011"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000132"/>
